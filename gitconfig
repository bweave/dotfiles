	name = Brian Weaver
	email = bdrums@gmail.com
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
	process = git-lfs filter-process
[core]
  excludesfile = ~/.gitignore
  commentChar = ";"
[commit]
  template = ~/.gitmessage
[rebase]
  autosquash = true
[pull]
  rebase = true
[push]
  default = current
[alias]
  stashdiff = stash show -
[init]
	defaultBranch = main
[hub]
	protocol = ssh
[fetch]
	prune = true
